      "source": [
        "# to count the no of numbers and alphabets and space\n",
        "def count(t):\n",
        " alpha =0\n",
        " space=0\n",
        " digit=0\n",
        "\n",
        "for i in t:\n",
        "  if i.isalpha():\n",
        "    alpha=alpha+1\n",
        "  if i.isspace():\n",
        "    space=space+1\n",
        "  if i.isdigit():\n",
        "    digit=digit+1\n",
        "  return alpha,space,digit"
      ]
    },
